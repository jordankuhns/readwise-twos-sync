# Readwise API Token
# Get this from: https://readwise.io/access_token
READWISE_TOKEN=your_readwise_token_here

# Twos API Credentials
# Get these from: Twos app → Settings → API
TWOS_USER_ID=your_twos_user_id_here
TWOS_TOKEN=your_twos_api_token_here

# Optional Configuration
# Number of days to look back for initial sync (default: 7)
SYNC_DAYS_BACK=7

# Path to file storing last sync timestamp (default: last_sync.json)
LAST_SYNC_FILE=last_sync.json